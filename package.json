    "@react-navigation/native-stack": "^7.5.1",
    "@reduxjs/toolkit": "^2.9.2",
    "i18next": "^25.6.0",
    "io-react-native-secure-storage": "^0.1.1",
    "lodash": "^4.17.21",
    "react": "19.0.0",
    "react-i18next": "^16.1.5",
    "react-native": "0.78.2",
    "react-native-device-info": "^14.1.1",
