{
  "name": "ageverification",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "tsc": "tsc --noEmit",
    "prettify": "prettier --write \"(ts|locales)/**/*.(ts|tsx|json)\"",
    "prettier:check": "prettier --check \"ts/**/*.(ts|tsx)\"",
    "lint-autofix": "eslint . -c .eslintrc.js --ext .ts,.tsx --fix",
    "nuke": "rm -rf ios/Pods && rm -rf ios/DerivedData && cd android && ./gradlew clean && cd .. && rm -rf node_modules",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write \"(ts|locales)/**/*.(ts|tsx|json)\"",
      "eslint ."
    ]
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5.2.6",
