    "@types/react-test-renderer": "^19.0.0",
    "@types/redux-persist": "^4.3.1",
    "eslint": "^8.19.0",
    "eslint-plugin-functional": "^6.6.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-sonarjs": "^1.0.4",
    "husky": "^9.1.7",
    "jest": "^29.6.3",
    "lint-staged": "^16.1.6",
    "prettier": "2.8.8",
